{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "Vehicle",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "brand": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "potency": {
      "type": "string",
      "required": true
    },
    "proprietary_type": {
      "type": "integer",
      "required": true
    },
    "color": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "year": {
      "type": "string",
      "required": true
    },
    "plate": {
      "type": "string",
      "regex": "([A-Z]{3}[0-9][0-9A-Z][0-9]{2})",
      "required": true,
      "unique": true
    },
    "vehicle_type": {
      "type": "string",
      "required": true
    },
    "fuel": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "date"
    },
    "gearshift_type": {
      "type": "string",
      "required": true
    },
    "equipment_name": {
      "type": "string",
      "required": true
    },
    "patrimony_code": {
      "type": "string",
      "required": true
    },
    "capacity": {
      "type": "string",
      "required": true
    },
    "owner_name": {
      "type": "string",
      "required": true
    },
    "registered_by": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
